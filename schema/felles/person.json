{
    "$schema": "http://json-schema.org/schema#",
    "$id": "https://fintlabs.no/schema/felles/person.json",
    "type": "object",
    "properties": {
        "bilde": {
            "type": "string"
        },
        "bostedsadresse": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/adresse.json"
        },
        "fodselsdato": {
            "type": "string", "format": "date-time"
        },
        "fodselsnummer": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/identifikator.json"
        },
        "navn": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/personnavn.json"
        },

        "kontaktinformasjon": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/kontaktinformasjon.json"
        },
        "postadresse": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/adresse.json"
        },
        "_links": {
            "type": "object",
            "properties": {

                "statsborgerskap": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "kjonn": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "malform": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "personalressurs": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "morsmal": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "parorende": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "elev": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "self": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                }
            },
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "href": {
                            "type": "string",
                            "format": "uri"
                        }
                    },
                    "required": [ "href" ]
            }
            },
            "required": [
                "self"
            ]
        }
    },
    "required": [
        "fodselsnummer", 
        "navn", 
        "_links"
    ]
}
