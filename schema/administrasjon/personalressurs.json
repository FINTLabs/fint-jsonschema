{
    "$schema": "http://json-schema.org/schema#",
    "$id": "https://fintlabs.no/schema/administrasjon/personalressurs.json",
    "type": "object",
    "properties": {
        "ansattnummer": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/identifikator.json"
        },
        "ansettelsesperiode": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/periode.json"
        },
        "ansiennitet": {
            "type": "string", "format": "date-time"
        },
        "brukernavn": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/identifikator.json"
        },
        "kontaktinformasjon": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/kontaktinformasjon.json"
        },
        "systemId": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/identifikator.json"
        },

        "_links": {
            "type": "object",
            "properties": {

                "personalressurskategori": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "arbeidsforhold": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "person": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "stedfortreder": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "fullmakt": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "leder": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "personalansvar": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "skoleressurs": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                },
	
                "self": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": [ "href" ]
                    }
                }
            },
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "href": {
                            "type": "string",
                            "format": "uri"
                        }
                    },
                    "required": [ "href" ]
            }
            },
            "required": [
                "personalressurskategori",
                "person",
                "self"
            ]
        }
    },
    "required": [
        "ansattnummer", 
        "ansettelsesperiode", 
        "_links"
    ]
}
