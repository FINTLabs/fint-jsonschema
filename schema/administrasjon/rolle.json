{
    "$schema": "http://json-schema.org/schema#",
    "$id": "https://fintlabs.no/schema/administrasjon/rolle.json",
    "type": "object",
    "properties": {
        "beskrivelse": {
            "type": "string"
        },
        "navn": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/identifikator.json"
        },

        "_links": {
            "type": "object",
            "properties": {

                "fullmakt": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                },
	
                "self": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "href": {
                            "type": "string"
                        }
                    }
            }
            },
            "required": [

                "fullmakt",
                "self"
            ]
        }
    },
    "required": [
        "beskrivelse",
        "navn",

        "_links"
    ]
}
