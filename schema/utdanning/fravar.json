{
    "$schema": "http://json-schema.org/schema#",
    "$id": "https://fintlabs.no/schema/utdanning/fravar.json",
    "type": "object",
    "properties": {
        "dokumentert": {
            "type": "boolean"
        },
        "foresPaVitnemal": {
            "type": "boolean"
        },
        "gjelderPeriode": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/periode.json"
        },
        "kommentar": {
            "type": "string"
        },
        "systemId": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/identifikator.json"
        },

        "_links": {
            "type": "object",
            "properties": {

                "elevforhold": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                },
	
                "undervisningsgruppe": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                },
	
                "eksamensgruppe": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                },
	
                "fravarstype": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                },
	
                "self": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "href": {
                            "type": "string"
                        }
                    }
            }
            },
            "required": [

                "elevforhold",
                "fravarstype",
                "self"
            ]
        }
    },
    "required": [
        "dokumentert", 
        "foresPaVitnemal", 
        "gjelderPeriode", 
        "kommentar", 
        "systemId", 
        "_links"
    ]
}
