{
    "$schema": "http://json-schema.org/schema#",
    "$id": "https://fintlabs.no/schema/utdanning/time.json",
    "type": "object",
    "properties": {
        "beskrivelse": {
            "type": "string"
        },
        "navn": {
            "type": "string"
        },
        "systemId": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/identifikator.json"
        },
        "tidsrom": {
            "type": "object", "$ref": "https://fintlabs.no/schema/felles/periode.json"
        },

        "_links": {
            "type": "object",
            "properties": {

                "undervisningsgruppe": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                },
	
                "undervisningsforhold": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                },
	
                "rom": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                },
	
                "self": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "href": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "additionalProperties": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "href": {
                            "type": "string"
                        }
                    }
            }
            },
            "required": [

                "undervisningsgruppe",
                "undervisningsforhold",
                "self"
            ]
        }
    },
    "required": [
        "navn",
        "systemId",
        "tidsrom",

        "_links"
    ]
}
